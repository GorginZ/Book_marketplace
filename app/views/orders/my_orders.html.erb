<h1> Your Orders </h1>

<div class= "container" >
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Author</th>
        <th scope="col">ISBN</th>
        <th scope="col">Price</th>
        <th scope="col">Purchased</th>

      </tr>
    </thead>
    <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td><%= order.listing.title %></td>
          <td><%= order.listing.author %></td>
          <td><%= order.listing.isbn %></td>
          <td><%= order.listing.price %></td>
          <td><%= order.listing.created_at %></td>


        </tr>
        <% end %>
    </tbody>
  </table>
</div>

